/dts-v1/;

#include "imx6dl-smarc.dtsi"
#include "imx6qdl-smarcfimx6.dtsi"
#include "imx6dl-smarc-common.dtsi"

/ {
	model = "Freescale i.MX6 DualLite SABRE Smart Device Board(PFUZE100)";
	compatible = "fsl,imx6dl-sabresd", "fsl,imx6dl", "fsl, imx6dl-smarcfimx6";
};

&ldb {
        lvds-channel@0 {
                crtc = "ipu1-di0";
        };

        lvds-channel@1 {
                crtc = "ipu1-di1";
        };
};

&mxcfb1 {
        status = "okay";
};

&mxcfb2 {
        status = "okay";
};

&pxp {
        status = "okay";
};

&vpu {
        pu-supply = <&reg_pu>; /* ldo-bypass:use pu_dummy if VDDSOC share with VDDPU */
};

&wdog1 {
        status = "okay";
};

&wdog2 {
        status = "disabled";
};
